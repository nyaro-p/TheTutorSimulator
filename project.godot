config_version=4

[application]

config/name="The Adem Simulator"
run/main_scene="res://Prefabs/UI/SplashScreen.tscn"
boot_splash/show_image=false
config/icon="res://AdemIcon.png"
config/windows_native_icon="res://AdemIcon.ico"

[autoload]

SaveFile="*res://Scenes/SceneScripts/SaveFile.gd"
GlobalAudio="*res://Scenes/GlobalAudio.tscn"
GlobalStats="*res://Scenes/GloabalScenes/GlobalStats.tscn"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/width=512
window/size/height=288
window/size/test_width=1024
window/size/test_height=576
window/stretch/mode="2d"
window/stretch/aspect="keep"

